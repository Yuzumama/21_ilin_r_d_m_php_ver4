button.book_btn_style {
    position: relative;
    width: 120px;
    height: 40px;
    background-color: white;
    border: 0px;
    margin-bottom: 30px;
    margin-right: 10px;
    font-size: 15px;
    font-family: Century Gothic;
    font-weight: bold;
    color: rgb(0, 0, 0);
    mix-blend-mode: darken;
    /* color: transparent; */
    /* -webkit-background-clip: text; */
    /* background-clip: text;  */
}

button.book_btn_style::after {
    content: "";
    position: absolute;
    width: 0px;
    left: 10%;
    bottom: 10px;
    border-bottom-width: 0px;
    border-bottom-style: solid;
    border-color: rgb(0,0,0);
    transition-duration: 1000ms;
}

button.book_btn_style:hover::after {
    content: "";
    width: 80%;
    border-bottom-width: 0.5px;
    border-bottom-style: solid;
    transition-duration: 1000ms;
}

button.book_btn_style:hover:active {
    transform: translate(3px, 3px);
}

