html {
    margin: 0;
    padding: 0;
    height: 100%;
    width: 100%;
}

body {
    margin: 0;
    padding: 0;
    height: 100%;
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    overflow: auto;
    background-image: url("../img/book_back_5.jpg"); max-width: 100%; height: auto;
    background-size: 1360px;
    background-position: 0px 0px;
    background-repeat: no-repeat;
}

/* Empty, for hidden list of the right-upper side */
.empty {
    position: absolute;
    left: 0;
    top: 0;
    width: 0px;
    height: 0px;
    opacity: 0.0;
    z-index: -1;
}


